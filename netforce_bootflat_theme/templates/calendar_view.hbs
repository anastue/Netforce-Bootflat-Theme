{{#unless options.widget}}
    <div class="page-header nf-page-header">
        <h1>{{options.string}}</h1>
    </div>
    {{view "flash" context=context}}
    <div class="btn-toolbar nf-btn-toolbar">
        {{{call render_top context}}}
    </div>
    {{#if tabs}}
        {{#view "tabs" context=context}}
            {{#each ../tabs context=context}}
                {{view "tab_item" string=string action=action action_options=action_opts context=context}}
            {{/each}}
        {{/view}}
    {{/if}}
{{/unless}}
<div class="search"></div>
<div class="calendar"></div>
